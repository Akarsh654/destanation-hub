            <script>
                var interests = ["carpenters", "logistics"]
                keywords = ['carpenter', 'healthy food', 'green house']


                $('#interests-select-dropdown').select2({
                    placeholder: 'Select an option',
                    dropdownParent: $('#select-body')
                }).select2({
                    placeholder: 'Select Your interests..',
                    data: interests
                });
                $("#interests-select-dropdown").append("<li> <span> " + "asdfsdfadfad" + " <i class='bx bx-x' ></i></span> </li>");


                $('#interests-select-dropdown').on('select2:select', function(e) {
                    var data = e.params.data;
                    console.log('data:', data)
                        //check if already in array, and do nothing

                    $("#selected-interest-options").append("<li> <span> " + data.text + " <i class='bx bx-x'></i></span> </li>");


                    $('#selected-interest-options li').each((index, interest) => {
                        $(interest).on('click', () => {
                            console.log('interest');
                            let id = interest.id;
                            interest.remove();
                        })
                    })
                });


                $('#keywords-select-dropdown').select2({
