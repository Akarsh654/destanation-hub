<!DOCTYPE html>
<html lang="en">

<head>
    <title>Destanation Hub</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="DirectoryPlus template project">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="index_resources/styles/bootstrap-4.1.2/bootstrap.min.css">
    <link href="index_resources/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="index_resources/plugins/OwlCarousel2-2.3.4/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="index_resources/plugins/OwlCarousel2-2.3.4/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="index_resources/plugins/OwlCarousel2-2.3.4/animate.css">
    <link rel="stylesheet" type="text/css" href="index_resources/styles/listings.css">
    <link rel="stylesheet" type="text/css" href="index_resources/styles/listings_responsive.css">
</head>

<body>

    <!-- Menu -->

    <div class="menu">
        <div class="menu_container text-right">
            <div class="menu_close">close</div>
            <nav class="menu_nav">
                <ul>
                    <li><a href="index.html">Home</a></li>
                    <li><a href="#">About us</a></li>
                    <li><a href="listings.html">Listings</a></li>
                    <li><a href="blog.html">News</a></li>
                    <li><a href="contact.html">Contact</a></li>
                </ul>
            </nav>
            <div class="menu_link">
                <a href="#">+Add Listing</a>
            </div>
        </div>
    </div>

    <div class="super_container">

        <!-- Header -->

        <header class="header">
            <div class="header_background">
                <div class="background_image" style="background-image:url(index_resources/images/header.jpg)"></div>
            </div>
            <div class="header_overlay"></div>
            <div class="header_content d-flex flex-row align-items-center justify-content-start">

                <!-- Logo -->
                <div class="logo"><a href="#">Destanation<span>Hub</span><span>+</span></a></div>

                <!-- Header Nav -->
                <div class="header_right d-flex flex-row align-items-center justify-content-start ml-auto">
                    <nav class="main_nav">
                        <ul class="d-flex flex-row align-items-center justify-content-start">
                            <li><a href="/">Home</a></li>
                            <li><a href="#">About us</a></li>
                            <li class="active"><a href="listings.html">Volunteers</a></li>
                            <li><a href="blog.html">Events</a></li>
                            <li><a href="contact.html">Contact</a></li>
                        </ul>
                    </nav>
                    <div class="add_listing text-center trans_200"><a href="/login">+Add Business</a></div>
                    <div class="log_reg">
                        <ul class="d-flex flex-row align-items-center justify-content-start">
                            <li><a href="/login">Login</a></li>
                            <li><a href="/login">Register</a></li>
                        </ul>
                    </div>
                    <div class="hamburger">
                        <i class="fa fa-bars trans_200"></i>
                    </div>
                </div>

            </div>
        </header>

        <div class="super_container_inner">
            <div class="super_overlay"></div>

            <!-- Listings -->

            <div class="listings container_custom">
                <div class="container">
                    <div class="row">

                        <!-- Listings Content -->
                        <div class="col-xl-6">
                            <div class="listings_content">

                                <!-- Listing Search -->
                                <div class="listing_search_container">
                                    <form action="#" class="search_form" id="search_form">
                                        <div class="d-flex flex-md-row flex-column align-items-start justify-content-start">
                                            <div>
                                                <input type="text" class="search_input" placeholder="What are you looking for?" required="required">
                                                <button class="search_button">Search</button>
                                            </div>
                                            <div>
                                                <input type="text" class="search_input" placeholder="Your Location" required="required">
                                            </div>
                                        </div>
                                    </form>
                                </div>

                                <!-- Listing Filter -->
                                <div class="listing_filter">
                                    <div class="section_title">
                                        <h1>Filter by type</h1>
                                    </div>
                                    <div class="listing_filter_container d-flex flex-row align-items-start justify-content-start flex-wrap">

                                        <!-- Listing Checkbox -->
                                        <div class="listing_checkbox">
                                            <label>Resturants
											<input type="checkbox" data-filter=".coffee">
											<span class="checkmark"></span>
										</label>
                                        </div>

                                        <!-- Listing Checkbox -->
                                        <div class="listing_checkbox">
                                            <label>Law
											<input type="checkbox" data-filter=".drinks">
											<span class="checkmark"></span>
										</label>
                                        </div>

                                        <!-- Listing Checkbox -->
                                        <div class="listing_checkbox">
                                            <label>Consultancy
											<input type="checkbox" data-filter=".food">
											<span class="checkmark"></span>
										</label>
                                        </div>

                                    </div>
                                </div>

                                <!-- Results -->
                                <div class="results_container">
                                    <div class="section_title">
                                        <h1><span>Results for: </span>SNS Catering</h1>
                                    </div>
                                    <div class="results grid">

                                        <% businesses.forEach(function(business) {%>
                                            <div class="grid-item result coffee">
                                                <div class="listing">
                                                    <div class="listing_image">
                                                        <div class="listing_icon">
                                                            <a href="listing.html"><img src="index_resources/images/loc.png" alt=""></a>
                                                        </div>
                                                        <img src="index_resources/images/listing_2.jpg" alt="">
                                                    </div>
                                                    <div class="listing_title_container">
                                                        <div class="listing_title"><a href=`details/<%= business._id %>`><%= business.businessName %></a></div>
                                                        <div class="listing_info d-flex flex-row align-items-center justify-content-between">
                                                            <div class="listing_rating">4,5</div>
                                                            <div class="listing_price">$$$</div>
                                                            <div class="listing_divider">|</div>
                                                            <div class="listing_type"><%= business.businessType %></div>
                                                            <div class="listing_divider">|</div>
                                                            <div class="listing_status">Closed</div>
                                                        </div>
                                                    </div>
                                                    <div class="listing_testimonial">
                                                        <div class="d-flex flex-row align-items-center justify-content-start">
                                                            <!-- <div class="testimonial_image"><img src="index_resources/images/user_2.jpg" alt=""></div> -->
                                                            <div class="testimonial_text">
                                                                <p><%= business.businessDescription %></p>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
    
                                         <% }); %>
                                      

                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Listings Map -->
                        <div class="col-xl-6">
                            <div class="listings_map">

                                <!-- Google Map -->

                                <div class="map">
                                    <div id="google_map" class="google_map">
                                        <div class="map_container">
                                            <div id="map"></div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </div>

            <!-- Footer -->

            <footer class="footer container_custom">
                <div class="container">
                    <div class="row">
                        <div class="col">
                            <div class="footer_container d-flex flex-md-row flex-column align-items-center justify-content-md-start justify-content-center">
                                <div class="copyright order-md-1 order-2">
                                    <!-- Link back to Destanation Hub can't be removed. Template is licensed under CC BY 3.0. -->
                                    Copyright &copy;
                                    <script>
                                        document.write(new Date().getFullYear());
                                    </script>
                                    </a> Destanation Hub</a>
                                    <!-- Link back to Destanation Hub can't be removed. Template is licensed under CC BY 3.0. -->
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>

    </div>

    <script src="index_resources/js/jquery-3.3.1.min.js"></script>
    <script src="index_resources/styles/bootstrap-4.1.2/popper.js"></script>
    <script src="index_resources/styles/bootstrap-4.1.2/bootstrap.min.js"></script>
    <script src="index_resources/plugins/greensock/TweenMax.min.js"></script>
    <script src="index_resources/plugins/greensock/TimelineMax.min.js"></script>
    <script src="index_resources/plugins/scrollmagic/ScrollMagic.min.js"></script>
    <script src="index_resources/plugins/greensock/animation.gsap.min.js"></script>
    <script src="index_resources/plugins/greensock/ScrollToPlugin.min.js"></script>
    <script src="index_resources/plugins/Isotope/isotope.pkgd.min.js"></script>
    <script src="index_resources/plugins/OwlCarousel2-2.3.4/owl.carousel.js"></script>
    <script src="index_resources/plugins/easing/easing.js"></script>
    <script src="index_resources/plugins/progressbar/progressbar.min.js"></script>
    <script src="index_resources/plugins/parallax-js-master/parallax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&key=AIzaSyCIwF204lFZg1y4kPSIhKaHEXMLYxxuMhA"></script>
    <script src="index_resources/js/listings.js"></script>
</body>

</html>